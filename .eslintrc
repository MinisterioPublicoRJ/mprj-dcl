{
  "extends": ["prettier", "airbnb", "plugin:react/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "prettier/prettier": ["warn"],
    "no-unused-vars": "warn",
    "class-methods-use-this": "off",
    "no-use-before-define": "off",
    "object-curly-newline": "warn",
    "linebreak-style": "off"
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint"
}
